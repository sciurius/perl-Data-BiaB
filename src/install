#
# you don't need to install biabconverter, but you might want to to 
# access it easily 
#
mkdir /usr/share/biabconverter
mkdir /usr/share/biabconverter/templates
cp * /usr/share/biabconverter
mv /usr/share/biabconverter/*.lyt /usr/share/biabconverter/templates
ln -s /usr/share/biabconverter/biabconverter /usr/bin/biabconverter
